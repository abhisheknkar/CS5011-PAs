function [ X ] = updateXfrommail( X, mailfile, skiplines )
% Update counts of the words occuring in the mail
    fid = fopen(mailfile,'r');
    
    tline = fgetl(fid);
    for i = 1:3
        tline = fgetl(fid);
        if i == 3
            words = strsplit(tline);
            
        end
    end
end

