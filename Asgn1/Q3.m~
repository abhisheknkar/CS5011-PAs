% plot(Xtrain1)

XtrainCen = Xtrain - repmat(mean(Xtrain,1), size(Xtrain,1), 1);
covariancex = (XtrainCen'*XtrainCen)./(size(Xtrain,1)-1);                 
[V D] = eigs(covariancex,1);
Xtrain1D = Xtrain*V;
plot(Xtrain1D)